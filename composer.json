{
    "name": "delboy1978uk/bone-http",
    "description": "HTTP package for Bone Framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Derek Stephen McLean",
            "email": "delboy1978uk@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-fileinfo": "*",
        "boneframework/contracts": "^v1.2",
        "delboy1978uk/barnacle": "^2.3",
        "delboy1978uk/bone-server":  "^1.2.0",
        "delboy1978uk/router": "^4.4",
        "laminas/laminas-diactoros": "^3.3",
        "psr/http-message": "^2.0",
        "psr/http-server-middleware": "^1.0"
    },
    "require-dev": {
        "delboy1978uk/dev-tools": "^v1.1",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "Bone\\Http\\": "src/",
            "BoneTest\\Http\\": "tests/unit"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BoneTest\\": "tests/unit/"
        }
    }
}
