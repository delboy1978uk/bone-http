{
    "name": "delboy1978uk\/bone-http",
    "description": "Http package for Bone MVC Framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Derek Stephen McLean",
            "email": "delboy1978uk@gmail.com"
        }
    ],
    "require": {
        "php": ">7.3",
        "delboy1978uk/barnacle": "^2.3",
        "delboy1978uk/bone-server":  "^1.2.0",
        "laminas/laminas-diactoros": "2.*",
        "psr/http-message": "^1.0",
        "psr/http-server-middleware": "^1.0"
    },
    "require-dev": {
        "codeception/codeception": "^4.1",
        "codeception/module-asserts": "^1.1",
        "roave\/security-advisories": "dev-latest",
        "delboy1978uk/bone-router": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Bone\\Http\\": "src/",
            "BoneTest\\Http\\": "tests/unit"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BoneTest\\": "tests/unit/"
        }
    }
}
